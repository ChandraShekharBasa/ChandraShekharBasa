@Override
    public PSOResponse getClientCreditDetails(String ecid) {
        PSOResponse psoResponse = null;
        if (wfdsEnabled) {
            psoResponse = new PSOResponse();
            String ucn = null;
            try {
                ucn = commonUtilityService.getUcn(ecid);
                log.info("UCN: {}", ucn);
            } catch (Exception e) {
                log.error("Exception while getting ucn : ", e);
            }
            WFDSResponse wfdsResponse = commonUtilityService.getClientFacilityList(ucn);
            List<FacilityInfo> facilityList = commonUtilityService.getFacilityInfo(wfdsResponse);
            if (!CollectionUtils.isEmpty(facilityList)) {
                psoResponse.setResult(facilityList.get(0));
            } else {
                psoResponse.setResult(null);
            }
        } else {
            String url = psoBaseUrl + "/api/commcard/credit-details?ecid=" + ecid;
            psoResponse = getPSOResponse(ecid, url);
        }
        return psoResponse;
    }
