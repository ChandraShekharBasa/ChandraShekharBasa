  @Override
  public CmlCardDocumentResponse uploadCMLCardPdf(String fileName, byte[] cmlCardPdf, String kycId) {
    HttpHeaders httpHeaders = new HttpHeaders();
    httpHeaders.set(HttpHeaders.CONTENT_TYPE, MediaType.MULTIPART_FORM_DATA_VALUE);

    DocumentUploadMetaData metadata = DocumentUploadMetaData.builder().caller(CALLER)
        .docTitle(fileName)
        .internalUseOnlyIndicator("F")
        .personalInformationClassCode("P")
        .build();

    MultiValueMap<String, Object> parameters = new LinkedMultiValueMap();
    parameters
        .add("file", new MultipartInputStreamFileResource(new ByteArrayInputStream(cmlCardPdf),
                                                          fileName));
    Gson gson = new Gson();
    parameters.add("metadata", gson.toJson(metadata));
    try {
      URI uri = new URIBuilder(docServiceUploadUrl).build();
      HttpEntity requestEntity = new HttpEntity<>(parameters, httpHeaders);
      logger.info("Making a call to SDX document service to upload Pdf: {} for kycId: {}",
                  docServiceUploadUrl, kycId);
      logger.info("SDX document service payload : {}", parameters);
      ResponseEntity<CmlCardDocumentResponse> response = cbxRestTemplate.exchange(uri, HttpMethod.POST, requestEntity, CmlCardDocumentResponse.class);
//      ResponseEntity<CmlCardDocumentResponse> response = cbxRestTemplate.postForEntity(uri, requestEntity, CmlCardDocumentResponse.class);
      if (response.getStatusCode().is2xxSuccessful()) {
        logger.info("CMLCard PDF file uploaded successfully for kycId: {}", kycId);
        return response.getBody();
      } else {
        logger.info("CMLCard PDF file upload operation failed for kycId: {}", kycId);
      }
      return null;
    } catch (HttpClientErrorException | HttpServerErrorException | URISyntaxException ex) {
      MDC.put(CommonConstants.ERROR_CODE, LoggingErrorCodes.DOC_ERROR);
      logger.error("Exception occurred while uploading CMLCard Pdf to SDX document service: ", ex);
      logger.info("Status code: {}", ex.getMessage());
    }
    return null;
  }
