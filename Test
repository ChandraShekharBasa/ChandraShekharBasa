@RequestMapping(value = "/navigator/employee/search", method = RequestMethod.GET)
    public ResponseEntity<PSOResponse> getEmployeeDetailsFromNavigator(@RequestParam("searchData") String searchData) {
        PSOResponse psoResponse = new PSOResponse();
        try {
            List<EmployeeDTO> employeeSearchResponse = commonUtilityService.getEmployeeDetails(searchData);
            log.info("employeeSearchResponse: {}", employeeSearchResponse);
            psoResponse.setResult(employeeSearchResponse);
        } catch (Exception e) {
            log.error("Exception while getting employeeSearchResponse from navigator : ", e);
        }
        return ResponseEntity.ok(psoResponse);
    }
