@Test
void testInitiateAsyncClientOnboardingOps_acceleratorCardFlow() throws Exception {
    Long kycId = 1L;
    String uuid = "uuid";
    String ipAddress = "127.0.0.1";
    boolean isReprocess = false;

    KYCInformationDTO kycInformation = mock(KYCInformationDTO.class);
    CmlCardBaseDTO cmlCardBaseDTO = mock(CmlCardBaseDTO.class);
    List<Product> products = List.of(new Product(COLTConstants.ACCELERATOR_CARD));

    when(kycInformationUtilityService.getKycInformation(kycId)).thenReturn(kycInformation);
    when(kycInformationUtilityService.isRegularCardRequest(kycId, kycInformation)).thenReturn(true);
    when(cmlCardProductService.getProductsForKYC(kycId)).thenReturn(products);

    serviceClass.initiateAsyncClientOnboardingOps(kycId, uuid, ipAddress, isReprocess);

    verify(kycInformationUtilityService, times(1)).getKycInformation(kycId);
    verify(cmlCardProductService, times(1)).getProductsForKYC(kycId);
    // Add other verifications depending on expected behavior
}
